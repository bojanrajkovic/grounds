{
  "$schema": "https://unpkg.com/knip@5/schema.json",
  "workspaces": {
    ".": {
      "entry": ["scripts/*.ts"],
      "project": ["scripts/**/*.ts"]
    },
    "packages/*": {
      "project": ["src/**/*.ts", "tests/**/*.ts"]
    },
    "packages/test-utils": {
      "project": ["src/**/*.ts"]
    },
    "examples": {
      "entry": ["**/*.ts"],
      "project": ["**/*.ts"]
    }
  },
  "ignoreBinaries": ["mdbook"],
  "ignore": ["mdbook/**", "docs/api/**", "**/dist/**"],
  "ignoreExportsUsedInFile": true
}
